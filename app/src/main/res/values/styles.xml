<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/customprimary</item>
        <item name="colorPrimaryDark">@color/customprimary_dark</item>
        <item name="colorAccent">@color/customaccent</item>
    </style>

    <style name="AppThemeCustom" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/yellowprimary</item>
        <item name="colorPrimaryDark">@color/yellowprimary_dark</item>
        <item name="colorAccent">@color/yellowaccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
